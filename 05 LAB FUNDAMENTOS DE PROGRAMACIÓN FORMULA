#include <iostream>
#include <stdlib.h>
#include <math.h>
using namespace std;
int main ()
{
	float a, b, c, x1, x2;
	cout <<"Ingrese la cantidad de a:" <<endl; 
	cin>> a; 
	cout <<"Ingrese la cantidad de b:" <<endl; 
	cin>> b; 
	cout <<"Ingrese la cantidad de c:" <<endl; 
	cin>> c; 
	x1= ((-b+sqrt(b*b-4*a*c))/2*a);
	x2= ((-b-sqrt(b*b-4*a*c))/2*a);
	cout<<"El resultado de x1:" <<x1<<endl;
	cout<<"El resultado de x2:" <<x2<<endl; 
	return 0;
}
